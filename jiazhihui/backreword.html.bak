	<script>
		window.onload = function() {
			var menu = document.querySelector('#menu'),
				block = document.querySelector('#block'),
				carte = document.querySelector('#carte'),
				close = document.querySelector('#close'),
				backShow = document.getElementById('backShow'),
				showLi = backShow.getElementsByTagName('li'),
				change = document.querySelector('#change'),
				flag = 1,
				changeFlag = 0,
				randomArr = [],
				temp_array = new Array();	// 新建一个数组,将传入的数组复制过来

			block.style.width = cw + 'px';
			block.style.height = ch + 'px';

			menu.onclick = function () {
				if(flag) {
					block.style.display = 'block';
					moveAction(carte, 'right', 0);
					flag = 0;
				}
			}
			close.onclick = block.onclick = function () {
				if (!flag) {
					block.style.display = 'none';
					moveAction(carte, 'right', -carte.offsetWidth);
					flag = 1;
				}
			}

			// 换一批
			change.onclick = function () {
				for(var len = 0; len < showLi.length; len++) {
					randomArr.push(showLi[len]);
				}
				randomArr = getArrayItems(randomArr, 6);

				for(var arrLen = 0; arrLen < randomArr.length; arrLen++) {
					backShow.appendChild(randomArr[arrLen]);
				}
			}

			function getArrayItems(arr, num) {
				//新建一个数组,将传入的数组复制过来,用于运算,而不要直接操作传入的数组;
				for (var index in arr) {
					temp_array.push(arr[index]);
				}
				//取出的数值项,保存在此数组
				var return_array = new Array();
				for (var i = 0; i < num; i++) {
					//判断如果数组还有可以取出的元素,以防下标越界
					if (temp_array.length > 0) {
						//在数组中产生一个随机索引
						var arrIndex = Math.floor(Math.random()*temp_array.length);
						//将此随机索引的对应的数组元素值复制出来
						return_array[i] = temp_array[arrIndex];
						//然后删掉此索引的数组元素,这时候temp_array变为新的数组
						temp_array.splice(arrIndex, 1);
					} else {
						for (var index in arr) {
							temp_array.push(arr[index]);
						}
					}
				}
				return return_array;
			}

 
			// 分页
			pagination({
				cont 		: 'pageCont1',
				pages 		: 'pages',
				prev 		: 'prev1',
				next 		: 'next1',
				activeName 	: 'active',
				pageNum 	: 10
			});

			pagination({
				cont 		: 'pageCont2',
				pages 		: 'pages',
				prev 		: 'prev2',
				next 		: 'next2',
				activeName 	: 'active',
				pageNum 	: 10
			});

			pagination({
				cont 		: 'pageCont3',
				pages 		: 'pages',
				prev 		: 'prev2',
				next 		: 'next3',
				activeName 	: 'active',
				pageNum 	: 10
			});
		}
	</script>
